            var listPhone = listPhoneModel.Where(x => x.brandid == item.id).ToList();

            if (listPhone.Count <= 0) { continue; }
            //var lstGroup = listPhone.Select(x => new
            //{
            //    group = x.group,
            //    groupterm = x.groupterm
            //}).DistinctBy(.ToList();

            var lstGroup = listPhone
